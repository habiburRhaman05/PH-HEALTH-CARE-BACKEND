model MedicalReport {
    id         String   @id @default(uuid())
    patientId  String
    reportName String
    reportLink String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
   patient Patient @relation(fields: [patientId],references: [id],onDelete: Cascade)
   @@index([patientId])
   @@map("medical_reports")

}
